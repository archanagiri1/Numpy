import numpy as np

print("NumPy working")


# 1.creating arrays

list1= [1,2,3,4]
print(list1)

arr_list1=np.array(list1)
print(arr_list1)

print(type(arr_list1))


list1=[1,2,3]
list2=[4,5,6]
arr_list1_2=np.array([list1,list2])
print(arr_list1_2)


list1=[1,2,3]
list2=[4,5,6]
list3=[7,8,9]
arr_list1_3=np.array([[list1,list2,list3]])
print(arr_list1_3)

print(arr_list1_3.ndim)




